<div class="nested-fields">
  <div class="col-md-12 mb-3">
    <%= f.text_field :ddd, placeholder: 'DDD', class:'col-md-1' %>
    <%= f.text_field :phone, placeholder: 'Telefone', class:'col-md-2' %>
    
    <%= link_to_remove_association f, class: 'btn btn-sm btn-danger pl-3 pr-3', title:'Remover Contato' do %>
      <b>-</b>
    <% end %>
  </div>
</div>
